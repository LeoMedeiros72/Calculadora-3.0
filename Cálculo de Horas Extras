      "cell_type": "code",
      "source": [
        "match = re.match(r'(\\d+)h(\\d+)', banco_horas)\n",
        "if match:\n",
        "    horas = int(match.group(1))\n",
        "    minutos = int(match.group(2))\n",
        "else:\n",
        "    raise ValueError(\"Formato inválido\")\n",
        "\n",
        "# Convertendo para minutos\n",
        "total_minutos = horas * 60 + minutos\n",
        "\n",
        "print(total_minutos)"
      ],
